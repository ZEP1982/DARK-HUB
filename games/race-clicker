local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

local Window = Library.CreateLib("Race Clicker | SeaHub","BloodTheme")

game:GetService("StarterGui"):SetCore("SendNotification",{

	Title = "Executed Race-Clicker", -- Required

	Text = "Welcome!", -- Required

})

-- Tabs

local T1 = Window:NewTab("Race Clicker") 

local T1S = T1:NewSection("Character Edit")

T1S:NewTextBox("Character Speed", "Speed", function(s)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

