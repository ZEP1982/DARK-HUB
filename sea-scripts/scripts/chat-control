local plr = game.Players.LocalPlayer
local chr = plr.Character or plr.CharacterAdded:Wait()
local UserInputService = game:GetService("UserInputService")
local Key = "E"
local Vis = true
local function Visibility(Var)
if Var then 
for _, Part in oairs(chr:GetDescendants())do 
if Part:IsA("BasePart") or Part:IsA("MeshPart")then 
part.Transparency = 0
chr.Head.face.Tranceparency = 0
chr.HumanoidRootPart.Tranceparency = 1
end
end
else
for _, in Pairs(char:GetDescendents())do 
if Part:IsA("BasePart") or Part:IsA("MeshPart") then
Part.Tranceparency = 1
chr.Head.face.Tranceparency = 1
end 
end
end 
end
UserInputService.InputBegan:Connec(function(Input,GameStuff)
if GameStuff then return end
if Input.KeyCode == Enum.KeyCode[Key] then 
if Visible then 
Visibility(false)
Vis = false 
else
Visibility(true)
Vis = true
end
end
end)
